{
    "name": "modelslab/modelq",
    "description": "A lightweight PHP task queue library, alternative to traditional queue systems, optimized for ML inference workloads",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "queue",
        "task-queue",
        "redis",
        "worker",
        "ml",
        "machine-learning",
        "inference",
        "async"
    ],
    "authors": [
        {
            "name": "Adhik Joshi",
            "email": "adhik@modelslab.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-redis": "*",
        "symfony/console": "^6.0|^7.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "ModelsLab\\ModelQ\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModelsLab\\ModelQ\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/modelq"
    ],
    "scripts": {
        "test": "phpunit",
        "analyse": "phpstan analyse src --level=6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
